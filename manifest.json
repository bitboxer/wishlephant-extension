{
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "manifest_version": 2,
  "version": "2.0.2",
  "background": {
    "scripts": ["build/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["build/content.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "icons/wishlephant-16.png",
      "32": "icons/wishlephant-32.png"
    }
  },
  "permissions": ["tabs"]
}
