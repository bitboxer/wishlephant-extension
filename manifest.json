{
  "description": "Add wishes to your wishlists on Wishlephant.com",
  "manifest_version": 2,
  "name": "Wishlephant",
  "version": "1.1.0",
  "background": {
    "scripts": ["src/background.js"]
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/wishlephant-16.png",
      "32": "icons/wishlephant-32.png"
    }
  },
  "permissions": [
    "tabs"
  ] 
}
